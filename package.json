    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "prisma": "^4.12.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "sveltekit-superforms": "^0.6.12",
